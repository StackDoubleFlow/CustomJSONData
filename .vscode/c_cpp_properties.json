{
  "configurations": [
    {
      "name": "Quest",
      "defines": [
        "__GNUC__",
        "__aarch64__",
        "VERSION=\"0.3.0\""
      ],
      "includePath": [
        "${workspaceFolder}/",
        "${workspaceFolder}/include",
        "${workspaceFolder}/shared",
        "${workspaceFolder}/extern",
        "${workspaceFolder}/extern/codegen/include",
        "${workspaceFolder}/extern/libil2cpp/il2cpp/libil2cpp",
        "${ANDROID_NDK_HOME}/toolchains/llvm/prebuilt/linux-x86_64/sysroot/usr/include",
        "${ANDROID_NDK_HOME}/sources/cxx-stl/llvm-libc++/include",
        "${ANDROID_NDK_HOME}/sources/cxx-stl/llvm-libc++abi/include"
      ],
      "compilerPath": "${ANDROID_NDK_HOME}/toolchains/llvm/prebuilt/linux-x86_64/bin/clang++",
      "cStandard": "gnu17",
      "cppStandard": "gnu++20",
      "intelliSenseMode": "clang-x64"
    }
  ],
  "version": 4
}